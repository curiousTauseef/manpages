	{ rank = same; 1964 runoff1 };
	{ rank = same; 1967 script runoff2 };
	{ rank = same; 1969 runoff3 };
	{ rank = same; 1971 roff };
	{ rank = same; 1972 nroff };
	{ rank = same; 1973 troff };
	{ rank = same; 1979 ditroff };
	{ rank = same; 1989 groff };
	{ rank = same; 1991 awf cawf };
